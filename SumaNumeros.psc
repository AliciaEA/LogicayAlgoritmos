Proceso SumaNumeros
	definir numero1 Como Entero;
	definir numero2 Como Entero;
	definir suma como entero;
	
	escribir "proporciona el valor del primer numero";
	leer numero1;
	
	escribir "proporciona el valor del segundo numero";
	leer numero2;
	
	suma <- numero1 + numero2;
	
	escribir "este es el resultado de la suma: ", suma;
FinProceso
